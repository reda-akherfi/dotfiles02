[38;2;117;113;94m#[0m[38;2;117;113;94m https://github.com/python/cpython/issues/73965[0m

[38;2;249;38;114mdef[0m[38;2;255;255;255m [0m[38;2;166;226;46mis_vanilla[0m[38;2;255;255;255m([0m[38;2;255;255;255m)[0m[38;2;255;255;255m [0m[38;2;255;255;255m->[0m[38;2;255;255;255m [0m[38;2;166;226;46mbool[0m[38;2;255;255;255m:[0m
[38;2;255;255;255m    [0m[38;2;117;113;94m"""[0m[38;2;117;113;94m :return: whether running "vanilla" Python [0m[38;2;117;113;94m"""[0m
[38;2;255;255;255m    [0m[38;2;249;38;114mimport[0m[38;2;255;255;255m [0m[38;2;255;255;255msys[0m
[38;2;255;255;255m    [0m[38;2;249;38;114mreturn[0m[38;2;255;255;255m [0m[38;2;249;38;114mnot[0m[38;2;255;255;255m [0m[38;2;102;217;239mhasattr[0m[38;2;255;255;255m([0m[38;2;255;255;255m__builtins__[0m[38;2;255;255;255m,[0m[38;2;255;255;255m [0m[38;2;230;219;116m'[0m[38;2;230;219;116m__IPYTHON__[0m[38;2;230;219;116m'[0m[38;2;255;255;255m)[0m[38;2;255;255;255m [0m[38;2;249;38;114mand[0m[38;2;255;255;255m [0m[38;2;230;219;116m'[0m[38;2;230;219;116mbpython[0m[38;2;230;219;116m'[0m[38;2;255;255;255m [0m[38;2;249;38;114mnot[0m[38;2;255;255;255m [0m[38;2;249;38;114min[0m[38;2;255;255;255m [0m[38;2;255;255;255msys[0m[38;2;255;255;255m.[0m[38;2;255;255;255margv[0m[38;2;255;255;255m[[0m[38;2;190;132;255m0[0m[38;2;255;255;255m][0m


[38;2;249;38;114mdef[0m[38;2;255;255;255m [0m[38;2;166;226;46msetup_history[0m[38;2;255;255;255m([0m[38;2;255;255;255m)[0m[38;2;255;255;255m:[0m
[38;2;255;255;255m    [0m[38;2;117;113;94m"""[0m[38;2;117;113;94m read and write history from state file [0m[38;2;117;113;94m"""[0m
