[38;2;255;255;255m    [0m[38;2;117;113;94m#[0m[38;2;117;113;94m https://specifications.freedesktop.org/basedir-spec/basedir-spec-latest.html#variables[0m
[38;2;255;255;255m    [0m[38;2;249;38;114mif[0m[38;2;255;255;255m [0m[38;2;255;255;255mstate_home[0m[38;2;255;255;255m [0m[38;2;249;38;114m:=[0m[38;2;255;255;255m [0m[38;2;255;255;255mos[0m[38;2;255;255;255m.[0m[38;2;255;255;255menviron[0m[38;2;255;255;255m.[0m[38;2;255;255;255mget[0m[38;2;255;255;255m([0m[38;2;230;219;116m'[0m[38;2;230;219;116mXDG_STATE_HOME[0m[38;2;230;219;116m'[0m[38;2;255;255;255m)[0m[38;2;255;255;255m:[0m
