[[1m[31mvim[0m]: [1m[3m/home/reda/.vimrc[0m
[38;2;255;255;255mSince 7.3.1178 vim will search for [0m[38;2;255;255;255m~[0m[38;2;255;255;255m/.vim/vimrc if [0m[38;2;255;255;255m~[0m[38;2;255;255;255m/.vimrc is not found.[0m

[38;2;255;255;255m"$XDG_CONFIG_HOME"/vim/vimrc[0m
[38;2;255;255;255m```[0m[38;2;190;132;255mvim[0m
[38;2;255;255;255mset runtimepath^=$XDG_CONFIG_HOME/vim[0m
[38;2;255;255;255mset runtimepath+=$XDG_DATA_HOME/vim[0m
[38;2;255;255;255mset runtimepath+=$XDG_CONFIG_HOME/vim/after[0m

[38;2;255;255;255mset packpath^=$XDG_DATA_HOME/vim,$XDG_CONFIG_HOME/vim[0m
[38;2;255;255;255mset packpath+=$XDG_CONFIG_HOME/vim/after,$XDG_DATA_HOME/vim/after[0m

[38;2;255;255;255mlet g:netrw_home = $XDG_DATA_HOME."/vim"[0m
[38;2;255;255;255mcall mkdir($XDG_DATA_HOME."/vim/spell", 'p')[0m

[38;2;255;255;255mset backupdir=$XDG_STATE_HOME/vim/backup | call mkdir(&backupdir, 'p')[0m
[38;2;255;255;255mset directory=$XDG_STATE_HOME/vim/swap   | call mkdir(&directory, 'p')[0m
[38;2;255;255;255mset undodir=$XDG_STATE_HOME/vim/undo     | call mkdir(&undodir,   'p')[0m
[38;2;255;255;255mset viewdir=$XDG_STATE_HOME/vim/view     | call mkdir(&viewdir,   'p')[0m

[38;2;255;255;255mif !has('nvim') | set viminfofile=$XDG_STATE_HOME/vim/viminfo | endif[0m
[38;2;255;255;255m```[0m

[38;2;255;255;255m~[0m[38;2;255;255;255m/.profile[0m
[38;2;255;255;255m```[0m
[38;2;255;255;255mexport GVIMINIT='let $MYGVIMRC="$XDG_CONFIG_HOME/vim/gvimrc" | source $MYGVIMRC'[0m
[38;2;255;255;255mexport VIMINIT='let $MYVIMRC="$XDG_CONFIG_HOME/vim/vimrc" | source $MYVIMRC'[0m
[38;2;255;255;255m```[0m
[38;2;255;255;255m[G]VIMINIT environment variable will also affect Neovim. If separate configs for Vim and Neovim are desired then the following will be a better choice:[0m

[38;2;255;255;255m```[0m
[38;2;255;255;255mexport GVIMINIT='let $MYGVIMRC = !has("nvim") ? "$XDG_CONFIG_HOME/vim/gvimrc" : "$XDG_CONFIG_HOME/nvim/init.gvim" | so $MYGVIMRC'[0m
[38;2;255;255;255mexport VIMINIT='let $MYVIMRC = !has("nvim") ? "$XDG_CONFIG_HOME/vim/vimrc" : "$XDG_CONFIG_HOME/nvim/init.vim" | so $MYVIMRC'[0m
[38;2;255;255;255m```[0m

